<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.plutonight.library.mapper.FootprintMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.plutonight.library.entity.Footprint">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="school_id" property="schoolId" />
        <result column="room_number" property="roomNumber" />
        <result column="seats_number" property="seatsNumber" />
        <result column="stay_time" property="stayTime" />
        <result column="status" property="status" />
        <result column="moment_tag" property="momentTag" />
        <result column="created_time" property="createdTime" />
        <result column="updated_time" property="updatedTime" />
        <result column="wanted_time" property="wantedTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, school_id, room_number, seats_number, stay_time, status, moment_tag, created_time, updated_time, wanted_time
    </sql>

</mapper>
